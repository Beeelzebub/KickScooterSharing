@{
    ViewData["Title"] = "Home Page";
}


<script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDDROLTz1lD3UOToVUJRiXR3BfRsdpQdRg&callback=initMap&libraries=&v=beta&map_ids=c59998b5dbf7aeb4"
        defer></script>
<link rel="stylesheet" href="~/css/map.css" />

<div class="row">
    <div id="map" class="float-left"> </div>
    <div id="order-menu" class="card">
        <div class="card-header text-center">
            <h5 class="mx-auto">Scooter info</h5>
        </div>
        <div class="card-body">
            <p>Name : 123</p>
            <p>Status : 123</p>
            <p>Tariff : 123</p>
        </div>
        <div class="card-footer btn-group">
            <div class="col">
                <button class="btn-block btn-lg btn-success">rent</button>
            </div>
            <div class="col">
                <button class="btn-block btn-lg btn-info">reserve</button>
            </div>
        </div>
    </div>
</div>

@section scripts {
    <script src="~/js/map.js" asp-append-version="true"></script>
}